  <div class="row">
    <div class="col-md-12 dashboard-container">
      {{#each posts}}
        <div class="panel panel-default" data-post="{{id}}">
          <div class="panel-heading">
            <h2>{{title}}</h2>
              <small>{{createdAt}}</small>
            </h2>
            <h5 style="float: right; color: blue; margin-top: -10px;">Condition: {{Condition.name}} </h5>
          </div>
          <div class="panel-body">
            <p>{{body}}</p>
          </div>
        </div>
      {{/each}}
    </div>
  </div>
</div>
<!-- Custom Script -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.17.0/moment.min.js" type="text/javascript"></script>
<script src="js/dashboard.js" type="text/javascript"></script>